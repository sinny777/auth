{
  "name": "auth",
  "version": "0.0.1",
  "description": "Authentication and Authorization",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.22.0",
    "npm": "7.21.0"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t auth .",
    "docker:run": "docker run -p 3000:3000 -d auth",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate --rebuild",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Gurvinder Singh - sinny777@gmail.com",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@loopback/authentication": "^7.3.2",
    "@loopback/authentication-passport": "^3.2.2",
    "@loopback/boot": "^3.4.2",
    "@loopback/core": "^2.16.2",
    "@loopback/logging": "^0.6.3",
    "@loopback/mock-oauth2-provider": "^0.3.2",
    "@loopback/repository": "^3.7.1",
    "@loopback/rest": "^10.0.0",
    "@loopback/rest-crud": "^0.11.0",
    "@loopback/rest-explorer": "^3.3.2",
    "@loopback/security": "^0.5.2",
    "@loopback/service-proxy": "^3.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/passport-google-oauth2": "^0.1.4",
    "@types/passport-http": "^0.3.9",
    "@types/passport-local": "^1.0.34",
    "@types/passport-oauth2": "^1.4.11",
    "@types/passport-twitter": "^1.0.37",
    "@types/uuid": "^8.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "client-sessions": "^0.8.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "jade": "^1.9.2",
    "loopback-connector-postgresql": "^5.4.0",
    "passport-google": "^0.3.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "path": "^0.12.7",
    "short-uuid": "^4.2.0",
    "tslib": "^2.0.0",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.1",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.172",
    "@types/node": "^10.17.60",
    "axios": "^0.21.4",
    "eslint": "^7.29.0",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.19",
    "typescript": "~4.3.4"
  }
}
